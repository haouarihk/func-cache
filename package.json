{
  "name": "func-cache",
  "version": "2.1.22",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "description": "TypeScript library for caching static functions",
  "keywords": [
    "cache",
    "redis",
    "memory",
    "upstash"
  ],
  "files": [
    "dist/**/*",
    "src/**/*",
    "tools/**/*"
  ],
  "engines": {
    "node": ">=10"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/haouarihk/func-cache.git"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "author": "haouarihk",
  "module": "dist/funcache.esm.js",
  "size-limit": [
    {
      "path": "dist/funcache.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/funcache.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^8.2.4",
    "@types/debounce": "^1.2.1",
    "@types/node": "^18.15.10",
    "@typescript-eslint/eslint-plugin": "4.23.0",
    "@typescript-eslint/parser": "4.23.0",
    "aforwait": "^1.2.0",
    "husky": "^8.0.3",
    "size-limit": "^8.2.4",
    "tsdx": "^0.14.1",
    "tslib": "^2.5.0",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "@upstash/redis": "^1.20.3",
    "debounce": "^1.2.1",
    "redis": "^4.6.5"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./src/index.ts",
      "require": "./dist/func-cache.cjs.production.min.cjs"
    },
    "./tools/fs": "./tools/fs.ts",
    "./tools/redis": "./tools/redis.ts",
    "./tools/upstash": "./tools/upstash.ts",
    "./tools": "./tools",
    "./d/types.d": "./d/types.d"
  }
}
